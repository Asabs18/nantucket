{
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        ".docs": "LICENSE, *.pdf, *.md, plot.txt",
        ".configs": "go.mod, go.sum, go.work, *.yaml, .gitignore, Makefile, .replit.txt, *.nix",
        ".data": "*.dat, *.xlsx",
        ".bins": "simulate, analyze, visualize",
        ".include": "*.h",
        ".src": "*.c"
    },
    "cSpell.words": [
        "Asab",
        "binno",
        "binsize",
        "curr",
        "dmax",
        "dmin",
        "dprob",
        "goroutines",
        "inputfile",
        "nbins",
        "ndays",
        "ndead",
        "newstatus",
        "nexposures",
        "ninfected",
        "npeople",
        "nrecovered",
        "nsusceptible",
        "ntrials",
        "nvaccinated",
        "otherperson",
        "outputfile",
        "overunderflow",
        "pthread",
        "pwrite",
        "SICKDAYS",
        "strs",
        "tprob",
        "unbuffered",
        "vprob"
    ],
    "workbench.colorCustomizations": {
        "activityBar.background": "#40272B",
        "titleBar.activeBackground": "#5A363C",
        "titleBar.activeForeground": "#FCF9FA",
        "tab.activeBorder": "#f5f9ad",
        "statusBar.background": "#f5f9ad",
        "statusBar.foreground": "#000000"
    },
    "kanban.saveFiles.pathPreferences": [
        ".vscode/kanban.json",
        "kanban.json"
    ],
    "kanban.statusButton.alignment": "Left",
    "window.title": "~[cmd]~${separator}${dirty}${activeEditorShort}${separator}${rootName}${separator}${appName}"
}